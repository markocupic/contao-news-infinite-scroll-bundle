{
  "name": "markocupic/contao-news-infinite-scroll-bundle",
  "type": "contao-bundle",
  "description": "Add Infinite Scroll to the Contao Newslist Module",
  "keywords": [
    "contao",
    "infinite",
    "scroll",
    "newslist",
    "news"
  ],
  "license": "LGPL-3.0+",
  "authors": [
    {
      "name": "Marko Cupic",
      "homepage": "https://github.com/markocupic/contao-news-infinite-scroll-bundle"
    }
  ],
  "require": {
    "contao/core-bundle": ">4.3"
  },
  "autoload": {
    "psr-4": {
      "Markocupic\\ContaoNewsInfiniteScrollBundle\\": "src/"
    },
    "classmap": [
      "src/Resources/contao/"
    ],
    "exclude-from-classmap": [
      "src/Resources/contao/config/",
      "src/Resources/contao/dca/",
      "src/Resources/contao/languages/",
      "src/Resources/contao/templates/"
    ]
  },
  "extra": {
    "contao-manager-plugin": "Markocupic\\ContaoNewsInfiniteScrollBundle\\ContaoManager\\Plugin"
  }
}
